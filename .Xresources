!
! .Xresources
!
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb

URxvt*font:              xft:inconsolata:size=16:antialias=true
URxvt*boldFont:          xft:inconsolata:bold:size=16:antialias=true
URxvt*internalBorder:    5
URxvt*externalBorder:    0
URxvt*saveLines:         8192
URxvt*scrollBar:         false
URxvt*scrollTtyOutput:   false
URxvt*scrollWithBuffer:  true
URxvt*scrollTtyKeypress: true
URxvt*scrollColor:       Red4
URxvt*cursorColor:       Red4
URxvt*letterSpace:       -1
URxvt*thickness:         1
URxvt*scrollstyle:       plain

*background: black
*foreground: white

*color0:  black
*color1:  maroon
*color2:  medium aquamarine
*color3:  moccasin
*color4:  dark slate blue
*color5:  rebecca purple
*color6:  light blue
*color7:  gray
*color8:  dark gray
*color9:  maroon
*color10: aquamarine
*color11: moccasin
*color12: dark slate blue
*color13: rebecca purple
*color14: light blue
*color15: white
