{
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " âœ— ",
  "diagnostic.virtualTextLines": 1,
  "diagnostic.enableSign": false,
  "diagnostic.enableMessage": "never",
  "codeLens.enable": false,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.bracketEnterImprove": false,
  "coc.preferences.snippets.enable": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "coc.source.snippets.enable": false,

  "list.maxHeight": 10,
  "list.autoResize": false,
  "list.extendedSearchMode": false,

  "rust-analyzer.enableEnhancedTyping": false,
  "rust-analyzer.enableCargoWatchOnStartup": true,
  "rust-analyzer.featureFlags": {
     "completion.insertion.add-call-parenthesis": false
  },
  "suggest.fixInsertedWord": false,
  "suggest.maxCompleteItemCount": 24,

  "languageserver": {
    "rust": {
      "trace.server": "verbose",
      "command": "ra_lsp_server",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  }
}
